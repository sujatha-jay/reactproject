{
    "$schema": "https://json.schemastore.org/tsconfig",
    //"extends": "./configs/ts-config-custom/tsconfig.base.json",
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true
    }
}
